{
    "extends": "standard",
    "plugins": [
        "standard",
        "promise"
    ],
    "env": {
      "jest": true
    },
    "globals":{
      "Raphael": true,
      "FusionCharts": true
    },
    "rules": {
      "one-var": ["off"],
      "unused-vars": ["warn"],
      "semi": ["error", "always"],
      "no-unused-vars": ["warn"]
    }
}
